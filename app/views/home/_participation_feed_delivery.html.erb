<% delivery = participation_feed_delivery %>
<% participation = delivery.feed_item.originator %>
<div class='feed-list' id='feed_delivery_<%= delivery.id %>'>
  <div class="feed-icon icon-friend"></div>
  <p><%= profile_link participation.participant %> 参加了活动 <%= event_link participation.event %></p>
  <div><%= link_to_remote 'x', :url => feed_delivery_url(delivery), :method => :delete %></div>
</div>
