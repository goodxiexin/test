<% delivery = character_feed_delivery %>
<% character = delivery.feed_item.originator %>
<div id='feed_delivery_<%= delivery.id %>'>
	<div><%= avatar character.user %></div>
	<div><%= profile_link character.user %>创建了人物角色<%= link_to character.name, edit_profile_url(character.user.profile) %></div>
	<div><%= link_to_remote 'x', :url => feed_delivery_url(delivery), :method => :delete %></div>
	<div><%= ftime delivery.created_at %></div>
</div>
