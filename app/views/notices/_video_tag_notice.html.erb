<% video = tag.video %>
<% link = "#{link_to_function video.title, "home_manager.read_notice('#{read_notice_url(notice)}', '#{form_authenticity_token}');window.open('/videos/#{video.id}');"}" %>
<li>
  <div class="feed-message">
    <%= link_to_function '', "home_manager.read_notice('#{read_notice_url(notice)}', '#{form_authenticity_token}')", :class => 'icon-active' %>
    <%= profile_link tag.poster %> 在视频 <%= link %> 里标记了你
  </div>
</li>
